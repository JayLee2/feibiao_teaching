/* pages/my_wallect/addcard/addcard.wxss */
page{
  background: #fafafa
}
.headertext
{
  line-height: 30px;
  font-size: 16px;
  padding: 0 10px
}
.display
{
  float: left;
}
.list
{
  float: left;
  width: 100%;
  border-bottom: 1px solid#ddd;
  line-height: 40px;
  border-radius: 4px;
}
.list1
{
  float: left;
  width: 100%;
  line-height: 40px;
  border-radius: 4px;
}
.alllist
{
  float: left;
  width: calc(100% - 40px);
  padding: 0px 10px;
  margin: 0px 10px;
  background: #fff;
  font-size: 15px;
}
.border
{
  border:1px solid#9fd4d2;
  margin: 7.5px 10px;
  padding: 0 0 0 5px;
  width: 60%;
}
.textdepict
{
  width: 20%;
}
.confirm
{
  position: fixed;
  bottom: 0;
  left: -10px;
  background: #9fd4d2;
  line-height: 40px;
  font-size: 16px;
  width: calc(100% + 10px);
  text-align: center;
}
.picker
{
  height: 25px;
  line-height: 25px;
}