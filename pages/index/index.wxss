.main{
  width: 700rpx;
  margin: auto;
}
swiper{
  width: 700rpx;
  height: 280rpx;
  margin: auto;
}
swiper-item{
  width: 100%;
  height:280rpx;
}
swiper image{
  width: 100%;
  height: 100% !important;
}
.float_left{
  float: left;
}
.float_right{
  float: right;
}
.img_box{
  width: 100rpx;
  height: 100rpx;
  border-radius: 50%;
  overflow: hidden;
}
.img_box image{
  width: 100%;
  height:100%;
  vertical-align: bottom;
}
.user_info{
  border-bottom: 1rpx solid #ccc;
  padding: 20rpx 20rpx;
}
.clear::after{
  content: "";
  display: block;
  clear: both;
}
.list{
  width: 33%;
  height: 188rpx;
  float: left;
  text-align: center;
}
.list image{
  width: 64rpx;
  height: 64rpx;
  display: block;
  margin:15rpx auto;
}

.hidePage{
  position: absolute;  
  top: 0%;  
  left: 0%;  
  width: 100%;  
  height: 100%;  
  background-color: black;  
  z-index: 9;  
  opacity: 0.5;  
  filter: alpha(opacity=70);  
}


.authBut{
  position: absolute;
  top: 200rpx;
  left: 50%;
  transform: translate(-50%,0);
  width: 410rpx;
  height: 520rpx;
  z-index: 10;  
}

.authorize{
  position: absolute;
  left: 50%;
  transform: translate(-50%,0);
  width: 410rpx;
  height: 520rpx;
  z-index: 10;  
}
.username
{
  display: inline-block;
  max-width: 170px;
  overflow: hidden;
  text-overflow:ellipsis;
  white-space: nowrap;
}
.mask
{
  position: fixed;
  top:0%;
  left: 0%;
  width: 100%;
  height: 100%;
  background: #9fd4d2;
  z-index: 100;
}
.contact_usfrom
{
  position: fixed;
  top:16%;
  left: 0;
  width: 100%;
  z-index: 1000;
}
.form
{
  width: 60%;
  margin: 0 auto;
}
.textarea
{
  border:1px solid#fff;
  border-radius: 4px;
  margin: 0px 0 40px;
  padding: 5px;
  width: 100%;
}
.submitbutton
{
  width: 60%;
  background: #2ca879;
  font-size: 15px;
  color: #fff;
  border:unset;
}
.noticetext
{
  
  position: fixed;
  bottom: 30%;
  left: 0;
  right: 0;
  color: #fff;
  font-size: 12px;
  text-align: center;
}